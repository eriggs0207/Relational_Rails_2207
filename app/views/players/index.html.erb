<%= link_to "Mlb Players", "/players" %>
<%= link_to "Mlb Teams", "/teams" %>
<h1>Players</h1>
<% @players.each do |player| %>
<h3><%= player.name %></h3>
<h5>Team: <%= player.team.city %> <%= player.team.name %></h5>
<p>Salary: <%= player.salary %></p>
<p>Free Agent Next Season: <%= player.free_agent %></p>
<% end %>
