<%= form_with url: "/teams/#{@team.id}/players", method: :post, local: true do |form|%>

<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :position %>
<%= form.text_field :position %>
<%= form.label :salary %>
<%= form.number_field :salary %>
<%= form.radio_button :free_agent, true%>
<%= form.label "free agent" %>
<%= form.radio_button :free_agent, false%>
<%= form.label "under contract" %>

<%= form.submit "Add Player" %>
<% end %>
